<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhigrith">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Abhigrith">
        <activity android:name=".parent.activities.ParentSelectorActivity" />
        <activity android:name=".parent.activities.ParentSelectionActivity" />
        <activity android:name=".parent.activities.ParentRegisterActivity" />
        <activity android:name=".parent.activities.ParentProfileActivity"/>
        <activity android:name=".parent.activities.ParentDashboardActivity" />
        <activity android:name=".parent.activities.ParentLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>